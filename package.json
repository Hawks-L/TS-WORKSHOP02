  "main": "index.js",
  "scripts": {
    "star:dev": "nodemon --watch src --ext ts --exec tsx src/main.ts",
    "start:prod": "node dist/main.ts",
    "build": "tsc -p tsconfig.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "vitest run test/e2e",
    "lint": "eslint . --ext .ts",
    "format": "prettier --check .",
    "format:write": "prettier --write .",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.(ts,js,json,md)": [
      "prettier --write"
    ],
    "src/**/*.ts": [
      "eslint --fix"
    ]
  },
  "keywords": [],
